{
    "name": "richardkeep/nova-timenow",
    "description": "A Laravel Nova card to display the current time from different timezones in the world.",
    "keywords": [
        "laravel",
        "nova",
        "card",
        "timezone",
        "datetime",
        "card"
    ],
    "homepage": "https://github.com/richardkeep/nova-timenow",
    "authors": [
        {
            "name": "Richard Keep",
            "email": "r.kipsang@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Richardkeep\\NovaTimenow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Richardkeep\\NovaTimenow\\TimenowServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
